{
	"manifest_version": 2,
	"name": "MIYUKKI Direct",
	"version": "0.0.0",
	"description": "A DJ MIYUKKI SYSTEM client .",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "icon19.png"
		},
		"default_title": "DJSYSTEM",
		"default_popup": "popup.html"
	},
	"permissions": [
    "https://dj.life-is-tech.com/api",
		"storage",
		"tabs",
    "identity"
	]
}
